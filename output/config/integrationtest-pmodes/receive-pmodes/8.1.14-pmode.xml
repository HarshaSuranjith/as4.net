<?xml version="1.0"?>
<PMode xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="eu:edelivery:as4:pmode">
  <Id>8.1.14-pmode</Id>
  <Mep>OneWay</Mep>
  <MepBinding>Pull</MepBinding>
  <Reliability>
    <DuplicateElimination>
      <IsEnabled>false</IsEnabled>
    </DuplicateElimination>
  </Reliability>
  <ReceiptHandling>
    <UseNNRFormat>false</UseNNRFormat>
    <ReplyPattern>Response</ReplyPattern>
    <SendingPMode>pmode</SendingPMode>
  </ReceiptHandling>
  <ErrorHandling>
    <NotifyMessageConsumer>false</NotifyMessageConsumer>
    <NotifyMethod />
    <UseSoapFault>false</UseSoapFault>
    <ReplyPattern>Response</ReplyPattern>
    <ResponseHttpCode>500</ResponseHttpCode>
    <SendingPMode>pmode</SendingPMode>
  </ErrorHandling>
  <ExceptionHandling>
    <NotifyMessageConsumer>false</NotifyMessageConsumer>
    <NotifyMethod />
  </ExceptionHandling>
  <Security>
    <SigningVerification>
      <Signature>Allowed</Signature>
    </SigningVerification>
  <Signing>
      <IsEnabled>false</IsEnabled>
      <KeyReferenceMethod>IssuerSerial</KeyReferenceMethod>
      <PrivateKeyFindValue>PartyA</PrivateKeyFindValue>
      <PrivateKeyFindType>FindBySubjectName</PrivateKeyFindType>
      <Algorithm>http://www.w3.org/2001/04/xmldsig-more#rsa-sha256</Algorithm>
      <HashFunction>http://www.w3.org/2001/04/xmlenc#sha256</HashFunction>
    </Signing>
    <Decryption>
      <Encryption>Ignored</Encryption>
      <PrivateKeyFindType>FindByThumbprint</PrivateKeyFindType>
    </Decryption>
  </Security>
  <MessagePackaging>
    <PartyInfo>
        <FromParty>
          <PartyIds>
            <PartyId>
              <Id>org:holodeckb2b:example:company:D</Id>
            </PartyId>
          </PartyIds>
          <Role>Sender</Role>
        </FromParty>
        <ToParty>
          <PartyIds>
            <PartyId>
              <Id>org:holodeckb2b:example:company:C</Id>
            </PartyId>
          </PartyIds>
          <Role>Receiver</Role>
        </ToParty>
      </PartyInfo>
      <CollaborationInfo>
        <ConversationId>org:holodeckb2b:example:conversation</ConversationId>
        <AgreementReference>
          <Name>http://agreements.holodeckb2b.org/examples/agreement0</Name>
        </AgreementReference>
        <Action>StoreMessage</Action>
        <Service>
          <Name>Examples</Name>
          <Type>org:holodeckb2b:services</Type>
        </Service>
    </CollaborationInfo>
  </MessagePackaging>
  <Deliver>
    <IsEnabled>true</IsEnabled>
     <PayloadReferenceMethod >
      <Type>FILE</Type>
      <Parameters>
          <Parameter name="location" value=".\messages\in\"/>
        </Parameters>
    </PayloadReferenceMethod>
    <DeliverMethod>
      <Type>FILE</Type>
      <Parameters>
          <Parameter name="location" value=".\messages\in\"/>
        </Parameters>
    </DeliverMethod>
  </Deliver>
</PMode>