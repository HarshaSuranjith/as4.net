<?xml version="1.0"?>
<PMode xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
  xmlns="eu:edelivery:as4:pmode">
  <Id>receive_bundled_message_pmode</Id>
  <Reliability>
    <DuplicateElimination>
      <IsEnabled>false</IsEnabled>
    </DuplicateElimination>
  </Reliability>
  <ReplyHandling>
    <ReplyPattern>Response</ReplyPattern>
    <ReceiptHandling>
      <UseNRRFormat>true</UseNRRFormat>
    </ReceiptHandling>
    <ErrorHandling>
      <UseSoapFault>false</UseSoapFault>
      <ResponseHttpCode>200</ResponseHttpCode>
    </ErrorHandling>
    <ResponseConfiguration>
      <Protocol>
        <Url>http://localhost:9090/msh</Url>
        <UseChunking>false</UseChunking>
        <UseHttpCompression>false</UseHttpCompression>
      </Protocol>
      <TlsConfiguration>
        <IsEnabled>false</IsEnabled>
        <TlsVersion>Tls12</TlsVersion>
      </TlsConfiguration>
    </ResponseConfiguration>
    <ResponseSigning>
      <IsEnabled>true</IsEnabled>
      <CertificateFindCriteria>
        <CertificateFindType>FindBySubjectName</CertificateFindType>
        <CertificateFindValue>AccessPointA</CertificateFindValue>
      </CertificateFindCriteria>
      <KeyReferenceMethod>BSTReference</KeyReferenceMethod>
      <Algorithm>http://www.w3.org/2001/04/xmldsig-more#rsa-sha256</Algorithm>
      <HashFunction>http://www.w3.org/2001/04/xmlenc#sha256</HashFunction>
    </ResponseSigning>
  </ReplyHandling>
  <ExceptionHandling>
    <NotifyMessageConsumer>true</NotifyMessageConsumer>
    <NotifyMethod>
      <Type>FILE</Type>
      <Parameters>
        <Parameter name="location" value=".\messages\exceptions\" />
      </Parameters>
    </NotifyMethod>
  </ExceptionHandling>
  <Security>
    <SigningVerification>
      <Signature>Ignored</Signature>
    </SigningVerification>
    <Decryption>
      <Encryption>Ignored</Encryption>
    </Decryption>
  </Security>
  <MessagePackaging>
    <CollaborationInfo>
      <AgreementReference>
        <Value>http://agreements.europa.org/agreement</Value>
      </AgreementReference>
      <Action>as4.net:receive_agent:bundling</Action>
      <Service>
        <Value>bundling</Value>
        <Type>as4.net:receive_agent:componenttest</Type>
      </Service>
    </CollaborationInfo>
  </MessagePackaging>
  <MessageHandling>
    <Deliver>
      <IsEnabled>true</IsEnabled>
      <PayloadReferenceMethod>
        <Type>FILE</Type>
        <Parameters>
          <Parameter name="location" value=".\messages\in\" />
        </Parameters>
      </PayloadReferenceMethod>
      <DeliverMethod>
        <Type>FILE</Type>
        <Parameters>
          <Parameter name="location" value=".\messages\in\" />
        </Parameters>
      </DeliverMethod>
    </Deliver>
  </MessageHandling>
</PMode>