<?xml version="1.0"?>
<PMode xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="eu:edelivery:as4:pmode">
  <Id>ComponentTest_ReceiveAgent_Forward</Id>
  <Mep>OneWay</Mep>
  <MepBinding>Push</MepBinding>
  <Reliability>
    <DuplicateElimination>
      <IsEnabled>false</IsEnabled>
    </DuplicateElimination>
  </Reliability>
  <ReplyHandling>
    <ReplyPattern>Response</ReplyPattern>
    <SendingPMode>unsigned-response-pmode</SendingPMode>
    <ReceiptHandling>
      <UseNNRFormat>false</UseNNRFormat>
    </ReceiptHandling>
    <ErrorHandling>
      <UseSoapFault>false</UseSoapFault>
      <ResponseHttpCode>500</ResponseHttpCode>
    </ErrorHandling>
  </ReplyHandling>
  <ExceptionHandling>
    <NotifyMessageConsumer>true</NotifyMessageConsumer>
    <NotifyMethod>
      <Type>FILE</Type>
      <Parameters>
        <Parameter name="location" value=".\messages\exceptions\" />
      </Parameters>
    </NotifyMethod>
  </ExceptionHandling>  
  <MessagePackaging>
    <PartyInfo>
      <FromParty>
        <PartyIds>
          <PartyId>
            <Id>org:eu:europa:as4:example:accesspoint:A</Id>
          </PartyId>
        </PartyIds>
        <Role>Sender</Role>
      </FromParty>
      <ToParty>
        <PartyIds>
          <PartyId>
            <Id>org:eu:europa:as4:example:accesspoint:B</Id>
          </PartyId>
        </PartyIds>
        <Role>Receiver</Role>
      </ToParty>
    </PartyInfo>
    <CollaborationInfo>
      <AgreementReference>
        <Value>http://agreements.europa.org/agreement</Value>
      </AgreementReference>
      <Action>Receive_Agent_Forwarding_Action</Action>
      <Service>
        <Value>Receive_Agent_Forwarding_Service</Value>
        <Type>eu:europa:services</Type>
      </Service>
    </CollaborationInfo>
  </MessagePackaging>   
  <MessageHandling>
    <Forward>
        <SendingPMode>PModeId_For_Forwarding</SendingPMode>
    </Forward>
  </MessageHandling>
</PMode>