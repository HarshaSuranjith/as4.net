<?xml version="1.0"?>
<PMode 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
  xmlns="eu:edelivery:as4:pmode">
  <Id>8.3.5-pmode</Id>
  <Mep>OneWay</Mep>
  <MepBinding>Pull</MepBinding>
  <Reliability>
    <DuplicateElimination>
      <IsEnabled>false</IsEnabled>
    </DuplicateElimination>
  </Reliability>
  <ReceiptHandling>
    <UseNNRFormat>true</UseNNRFormat>
    <ReplyPattern>Response</ReplyPattern>
    <SendingPMode>pmode</SendingPMode>
  </ReceiptHandling>
  <ErrorHandling>
    <NotifyMessageConsumer>true</NotifyMessageConsumer>
    <NotifyMethod>
      <Type>FILE</Type>
      <Parameters>
        <Parameter name="location" value=".\messages\errors\"/>
      </Parameters>
    </NotifyMethod>
    <UseSoapFault>false</UseSoapFault>
    <ReplyPattern>Response</ReplyPattern>
    <ResponseHttpCode>500</ResponseHttpCode>
    <SendingPMode>pmode</SendingPMode>
  </ErrorHandling>
  <ExceptionHandling>
    <NotifyMessageConsumer>false</NotifyMessageConsumer>
    <NotifyMethod />
  </ExceptionHandling>
  <Security>
    <SigningVerification>
      <Signature>Allowed</Signature>
    </SigningVerification>
    <Decryption>
      <Encryption>Allowed</Encryption>
      <PrivateKeyFindValue>AccessPointB</PrivateKeyFindValue>
      <PrivateKeyFindType>FindBySubjectName</PrivateKeyFindType>
    </Decryption>
  </Security>
  <MessagePackaging>
    <PartyInfo>
      <FromParty>
        <PartyIds>
          <PartyId>
            <Id>org:holodeckb2b:example:company:A</Id>
          </PartyId>
        </PartyIds>
        <Role>Sender</Role>
      </FromParty>
      <ToParty>
        <PartyIds>
          <PartyId>
            <Id>org:eu:europa:as4:example</Id>
          </PartyId>
        </PartyIds>
        <Role>Receiver</Role>
      </ToParty>
    </PartyInfo>
    <CollaborationInfo>
      <AgreementReference>
        <Name>http://agreements.holodeckb2b.org/examples/agreement0</Name>
      </AgreementReference>
      <Action>http://docs.oasis-open.org/ebxml-msg/ebMS/v3.0/ns/core/200704/test</Action>
      <Service>
        <Name>http://docs.oasis-open.org/ebxml-msg/ebMS/v3.0/ns/core/200704/service</Name>
        <Type>org:holodeckb2b:services</Type>
      </Service>
    </CollaborationInfo>
  </MessagePackaging>
  <Deliver>
    <IsEnabled>true</IsEnabled>
    <PayloadReferenceMethod >
      <Type>FILE</Type>
      <Parameters>
        <Parameter name="location" value=".\messages\in\"/>
      </Parameters>
    </PayloadReferenceMethod>
    <DeliverMethod>
      <Type>FILE</Type>
      <Parameters>
        <Parameter name="location" value=".\messages\in\"/>
      </Parameters>
    </DeliverMethod>
  </Deliver>
</PMode>